---
import Card from "../components/Card.astro";
import Shield from "../components/Shield.astro";
import Paragraph from "../components/Paragraph.astro";
import AgeDisplay from "../components/AgeDisplay.jsx";
import Contact from "../components/Contact.astro";

const { title, description, frontmatter } = Astro.props;
---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Astro description" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>DereWah</title>
	</head>
	<body class="bg-gradient-to-b from-ebony-clay-950 to-ebony-clay-900">
		<div class="px-10 py-14">
			<main class="flex flex-col w-full items-center">
				<div class="max-w-[65ch]">
					<slot />
				</div>
			</main>
		</div>
	</body>
</html>
